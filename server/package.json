{
	"name": "restaurant-tracker",
	"version": "0.1.0",
	"description": "Restaurant Tracking app",
	"main": "build/src/index.js",
	"scripts": {
		"start:server": "ts-node-dev src/index.ts",
		"start:codegen": "graphql-codegen --watch",
		"start:test": "node -r ts-node/register --test --watch src/**/*.spec.ts",
		"start": "node build/src/index.js",
		"dev": "run-p -l start:codegen start:server",
		"test": "run-p -l start:codegen start:test",
		"test:once": "node -r ts-node/register --test src/**/*.spec.ts",
		"codegen": "graphql-codegen",
		"build": "rimraf build && tsc && tsc-alias",
		"postbuild": "copyfiles -u 1 \"src/**/*.graphql\" build/src --soft",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix"
	},
	"author": {
		"name": "RaviAnand Mohabir",
		"url": "https://www.ravianand.me"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dan6erbond/restaurant-tracker.git"
	},
	"dependencies": {
		"@envelop/graphql-modules": "^7.0.1",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.39.3",
		"graphql": "16.10.0",
		"graphql-import-node": "^0.0.5",
		"graphql-modules": "^3.0.0",
		"graphql-scalars": "1.24.1",
		"graphql-yoga": "5.12.0",
		"pg": "^8.13.3",
		"reflect-metadata": "^0.2.2"
	},
	"devDependencies": {
		"@eddeee888/eslint-plugin": "0.0.2",
		"@eslint/compat": "^1.2.6",
		"@eslint/js": "^9.20.0",
		"@graphql-codegen/cli": "5.0.5",
		"@graphql-codegen/graphql-modules-preset": "^4.0.14",
		"@graphql-codegen/typescript": "^4.1.4",
		"@graphql-codegen/typescript-resolvers": "^4.4.3",
		"@graphql-typed-document-node/core": "3.2.0",
		"@parcel/watcher": "2.5.1",
		"@types/node": "^22.13.4",
		"@types/pg": "^8.11.11",
		"@typescript-eslint/eslint-plugin": "8.24.1",
		"@typescript-eslint/parser": "8.24.1",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"drizzle-kit": "^0.30.4",
		"eslint": "8.57.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"globals": "^15.15.0",
		"npm-run-all2": "7.0.2",
		"prettier": "^3.5.1",
		"rimraf": "^6.0.1",
		"ts-node": "10.9.2",
		"ts-node-dev": "2.0.0",
		"tsc-alias": "^1.8.10",
		"typescript": "5.7.3",
		"typescript-eslint": "^8.24.1"
	}
}
